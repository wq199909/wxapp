/* pages/record/record.wxss */
.container{
  height: 100vh;
  width: 750rpx;
  background: linear-gradient(to bottom right,rgb(64,153,235),rgb(76,85,164));
}
.container .main{
  margin:5vh 25rpx;
  height: 80vh;
  width: 700rpx;
  overflow: auto;
}
button{
  width: 400rpx;
  margin: 50rpx auto;
  height: 100rpx;
}
.success{
  color: black;
  padding:10rpx;
  height: 150rpx;
  border: 2rpx solid black;
  box-shadow: 0 1rpx 0 rgba(255,255,255,0.24), 0 1rpx 6px rgba(0,0,0,0.45) inset;
  background-color: #fff;
  margin-bottom: 20rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size:100rpx 100rpx;
  background-position: right center;
}
.fail{
  color: red;
  background-color: #fff;
  margin-bottom: 20rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size:100rpx 100rpx;
  background-position: right center;
  box-shadow: 0 1rpx 0 rgba(255,255,255,0.24), 0 1rpx 6px rgba(0,0,0,0.45) inset;
  border: 2rpx solid black;
  padding:10rpx;
  height: 150rpx;
}
image{
  display: inline-block;
  height: 100rpx;
  width: 100rpx;
  border-radius: 50rpx;
  margin-top: 25rpx;
  margin-right: 40rpx;
}
.box{
  display: inline-block;
  height: 150rpx;
  width:400rpx;
}
.name{
text-align: center;
height: 74rpx;
width: 400rpx;
line-height: 74rpx;
border-bottom: 1rpx dashed black;
}
.duration{
text-align: center;
height: 75rpx;
width: 400rpx;
line-height: 75rpx;
}
